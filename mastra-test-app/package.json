{
  "name": "mastra-test-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "mastra dev --inspect",
    "dev:pretty": "mastra dev --inspect | npx pino-pretty",
    "build": "mastra build && npm run build:prisma",
    "build:prisma": "cp -r prisma .mastra/output/ && cd .mastra/output && npm pkg set scripts.postinstall='prisma generate' && npm pkg set dependencies.prisma='6.12.0' && pnpm install",
    "postinstall": "prisma generate",
    "start": "mastra start",
    "seed": "tsx src/seed.ts",
    "seed:wic": "tsx src/seed-wic.ts",
    "seed:csv": "tsx src/seed-csv.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.23",
    "@inquirer/prompts": "^7.7.1",
    "@mastra/core": "^0.11.1",
    "@mastra/libsql": "^0.11.2",
    "@mastra/loggers": "^0.10.4",
    "@mastra/mcp": "^0.10.7",
    "@mastra/memory": "^0.11.5",
    "@prisma/client": "^6.12.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.1",
    "pg": "^8.16.3",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.12.0",
    "tsx": "^4.20.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.4",
    "mastra": "^0.10.15",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "form-data": ">=4.0.4"
  }
}